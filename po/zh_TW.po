#: pg_stat_insights.c
msgid "pg_stat_insights: Advanced query performance monitoring for PostgreSQL"
msgstr "pg_stat_insights: PostgreSQL進階查詢效能監控"

#: pg_stat_insights.c
msgid "Tracks execution statistics of all SQL statements executed by the server"
msgstr "追蹤伺服器執行的所有SQL語句的執行統計資訊"

#: pg_stat_insights.c
msgid "Could not initialize shared memory for pg_stat_insights"
msgstr "無法為pg_stat_insights初始化共用記憶體"

#: pg_stat_insights.c
msgid "pg_stat_insights must be loaded via shared_preload_libraries"
msgstr "pg_stat_insights必須透過shared_preload_libraries載入"

#: pg_stat_insights.c
msgid "pg_stat_insights must be loaded via \"shared_preload_libraries\""
msgstr "pg_stat_insights必須透過\"shared_preload_libraries\"載入"

#: pg_stat_insights.c
msgid "Insufficient shared memory for pg_stat_insights"
msgstr "pg_stat_insights的共用記憶體不足"

#: pg_stat_insights.c
msgid "Query text length exceeds maximum"
msgstr "查詢文字長度超過最大值"

#: pg_stat_insights.c
msgid "Successfully reset pg_stat_insights statistics"
msgstr "成功重設pg_stat_insights統計資訊"

#: pg_stat_insights.c
msgid "Invalid query ID for reset operation"
msgstr "重設操作的查詢ID無效"

#: pg_stat_insights.c
msgid "Statistics collection is disabled"
msgstr "統計資訊收集已停用"

#: pg_stat_insights.c
msgid "Extension initialization complete"
msgstr "擴充功能初始化完成"

#: pg_stat_insights.c
msgid "pg_stat_insights: out of memory for query entry"
msgstr "pg_stat_insights: 查詢項目記憶體不足"

#: pg_stat_insights.c
msgid "pg_stat_insights: normalized query buffer overflow, truncating"
msgstr "pg_stat_insights: 正規化查詢緩衝區溢位，正在截斷"

#: pg_stat_insights.c
msgid "pg_stat_insights: normalized query buffer overflow at end"
msgstr "pg_stat_insights: 正規化查詢緩衝區在結尾溢位"

#: pg_stat_insights.c
msgid "pg_stat_insights: index size tracking not initialized"
msgstr "pg_stat_insights: 索引大小追蹤未初始化"

#: pg_stat_insights.c
msgid "pg_stat_insights: index lock tracking not initialized"
msgstr "pg_stat_insights: 索引鎖定追蹤未初始化"

#: pg_stat_insights.c
msgid "incorrect number of output arguments"
msgstr "輸出參數數量不正確"

#: pg_stat_insights.c
msgid "return type must be a row type"
msgstr "返回類型必須是行類型"

#: pg_stat_insights.c
msgid "out of memory"
msgstr "記憶體不足"

#: pg_stat_insights.c
msgid "Could not allocate enough memory to read file \"%s\"."
msgstr "無法分配足夠的記憶體來讀取檔案\"%s\"。"
