msgid "Insufficient shared memory for pg_stat_insights"
msgstr "pg_stat_insights的共用記憶體不足"

#: pg_stat_insights.c
msgid "Query text length exceeds maximum"
msgstr "查詢文字長度超過最大值"

#: pg_stat_insights.c
msgid "Successfully reset pg_stat_insights statistics"
msgstr "成功重設pg_stat_insights統計資訊"

#: pg_stat_insights.c
msgid "Invalid query ID for reset operation"
msgstr "重設操作的查詢ID無效"

#: pg_stat_insights.c
msgid "Statistics collection is disabled"
msgstr "統計資訊收集已停用"

#: pg_stat_insights.c
msgid "Extension initialization complete"
msgstr "擴充功能初始化完成"

